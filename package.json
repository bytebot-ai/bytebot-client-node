{
  "name": "@bytebot/sdk",
  "version": "0.4.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "tsc",
    "prepack": "cp -rv dist/. ."
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/qs": "^6.9.11",
    "@types/url-join": "4.0.1",
    "axios": "^1.6.7",
    "form-data": "^4.0.0",
    "jest-cli": "^29.7.0",
    "jsdom": "^24.0.0",
    "qs": "^6.11.2",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.3.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "puppeteer": "^21.9.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "puppeteer": "^21.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "npm@10.2.4"
}
